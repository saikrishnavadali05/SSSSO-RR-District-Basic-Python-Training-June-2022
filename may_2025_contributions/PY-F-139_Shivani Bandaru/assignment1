print(f"Your favorite color is {color}.")

print(1, 2, 3, 4, 5, sep=', ')

def square(n):
    return n * n
for i in range(1, 6):
    print(f"Square of {i} is {square(i)}")

for i in range(1, 6):
    print('*' * i)

