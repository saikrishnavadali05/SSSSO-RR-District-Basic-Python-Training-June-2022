print(age >= 18)

a = input("Enter first Boolean value (True/False): ") == "True"
b = input("Enter second Boolean value (True/False): ") == "True"
print("a and b:", a and b)
print("a or b:", a or b)
print("not a:", not a)
print("not b:", not b)

values = [0, '', 42]
for value in values:
    if value:
        print(f"{value!r} is truthy")
    else:
        print(f"{value!r} is falsy")

