a=5
b=4
print(a < 0 and b < 0)
print(a > 0 or b > 0 )
print(not(a != b))
print(a == 0 and b != 0)
print(a != 0 and b != 0)
print(a == 0 and b == 0)
print(a == 0 or b != 0)
print(a != 0 or b != 0)
print(a == 0 or b == 0)
print(not(a == 0, b != 0))
print(not(a != 0, b != 0))
print(not(a == 0, b == 0))